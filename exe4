#include <stdio.h>
#include <stdlib.h>

int main ()
{
	int vet[15],i,s,s2;
	i=0;
	while (i<=15){
		scanf ("%d", &vet[i]);
		if (i<=6)
		{
			s=s+vet[i];
		}
		if (i >= 9)
		{    
			s2=s2+vet[i];
		}
		i++;
	}
		if ((s == s2) && (vet[7] == vet[8]))
		{
			printf ("\ne palindromo");
		}
		else
		{
			printf ("\nnao e palindromo");
		}
	return 0;
}
