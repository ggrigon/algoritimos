#include <stdio.h>
#include <stdlib.h>
int main ()
{
	int i,i2,x;
	float vetor[10];
		for (i=0;i<10;i++)
	{
		scanf ("%f", &vetor[i]);
	}
	for (i=0;i<10;i++){
		for (i2=0;i2<10;i2++){
			if (vetor[i]>=vetor[i2]){
				x=vetor[i];
				vetor[i]=vetor[i2];
				vetor[i2]=x;
			}
		}
	}
	for (i=10;i>-1;i--)
	{
		printf ("%f  ", vetor[i]);
	}	
	return 0;
}
