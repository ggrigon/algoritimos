#include <stdio.h>
#include <stdlib.h>
int main ()
{ 
    int  voto,valibaba,contagem,votoalcapone,votoalibaba,votonulo;
    votoalibaba=0;
    votoalcapone=0;
    printf ("insira seu voto:");
    for (contagem=1;contagem<10;contagem++){
    	printf ("\n");
        scanf ("%d", &voto);
        if (voto==83)
        {
        	if (voto==83){
        		printf ("\ndigite novamente seu voto para confirmar");
        		printf ("\n");
        		scanf ("\n%d", &voto);
			}
			else 
			{
				printf ("\nvoto nao computado");
			}
			if (voto==83)
			{
			votoalibaba++;
            printf("\nparabens voto computatado voce votou em alibaba:");
            printf ("\n====================novo voto================");
            printf("\n");
			}
            printf("\ninsira seu voto");
        }
        if (voto==93)
        {
        	if (voto==93){
        		printf ("\ndigite novamente seu voto para confirmar");
        		printf ("\n");
        		scanf ("\n%d", &voto);
			}
			else 
			{
				printf ("\nvoto nao computado");
				votoalcapone--;
			}
            votoalcapone++;
            printf("\nparabens voto computatado voce votou em alcapone:");
            printf ("\n====================novo voto================");
            printf("\n");
            printf("\ninsira seu voto");
        }
        if (voto == 0)
		{
        	printf ("\ndigite novamente para confirmar seu voto");
        	printf ("\n");
			scanf ("%d", &voto);
			if (voto == 0)
			{
				printf ("\nvoto nulo computado");
		 		printf ("\n====================novo voto================");
			}
			else
			{
				printf ("\nvoto nulo nao computado");
				printf ("\n________");
				printf	("\ninsira seu voto");
				votonulo--;
			}	
        
        	votonulo++;
		} 
    }
        if (votoalibaba > votoalcapone){
        	printf ("\n_____________________");
        	printf ("\nfim da votacao");
            printf ("\n!!!alibaba ganhou!!!");
            printf ("\n_____________________");
            printf ("\ncontagem:   ");
            printf ("\nalibaba %d  ", votoalibaba);
			printf ("\nalcapone %d  ", votoalcapone);
			printf ("\nvotos em branco %d  ", votonulo); 
        }
        else{
        	printf ("\n_____________________");
        	printf ("\nfim da votacao");
            printf ("\n!!!alcapone ganhou!!!");
            printf ("\n_____________________");
            printf ("\ncontagem:   ");
            printf ("\nalibaba %d  ", votoalibaba);
			printf ("\nalcapone %d  ", votoalcapone);
			printf ("\nvotos em branco %d  ", votonulo); 
    } 
    return 0;
}
